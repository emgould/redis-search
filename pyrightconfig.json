{
  "include": [
    "src",
    "web",
    "scripts"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/venv",
    "**/.venv",
    "**/tests",
    "**/.pytest_cache"
  ],
  "extraPaths": [
    "src",
    "."
  ],
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}


